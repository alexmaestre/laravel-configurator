{
    "name": "alexmaestre/laravel-configurator",
    "description": "This package allows to configure DB and Key in new Laravel installations using terminal.",
    "authors": [
        {
            "name": "Alex Maestre",
            "email": "contacto@vivazzio.com"
        }
    ],
	"require": {
        "php": ">=7.0.0"
    },
    "autoload": {
        "psr-4": {
		"LaravelConfigurator\\": "src/"
        }
    },	
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
		          "LaravelConfigurator\\Providers\\LaravelConfiguratorServiceProvider"
            ]
        }
    }	
}
